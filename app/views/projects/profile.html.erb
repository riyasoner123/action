<div class="managerForms">
<div class="text-center">
  <h3>Manager profile</h3>
</div>
  <div class="row">
    <div class="col">
      <h5>Name:-</h5>
    </div>
    <div class="col">
      <h6><%=@manager.name%></h6>
    </div>
  </div>

  <div class="row">
    <div class="col">
      <h5>Contact No:-</h5>
    </div>
    <div class="col">
      <h6><%=@manager.contact%></h6>
    </div>
  </div>

  <div class="row">
    <div class="col">
      <h5>Email:-</h5>
    </div>
    <div class="col">
      <h6><%=@manager.email%></h6>
    </div>
  </div>

  <div class="text-center">
    <%= link_to "Edit profile", edit_manager_registration_path %>
  </div>
  
</div>